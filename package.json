{
  "name": "typescript-backend-cdk-starter",
  "version": "1.0.0",
  "description": "Whire Backend",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "utils/*"
    ]
  },
  "scripts": {
    "pretty": "yarn prettier --write './{utils,packages}/**/src/**/*.ts'",
    "typecheck": "yarn lerna run typecheck --parallel",
    "start": "lerna run start --stream --parallel",
    "build": "lerna run build",
    "deploy": "yarn run build && yarn pre:deploy && yarn cdk:deploy",
    "pre:deploy": "lerna run pre:deploy --concurrency 1",
    "cdk:synth": "lerna run synth --scope @typescript-backend-cdk-starter/cdk-app && rm -rf cdk.out && cp -r packages/cdk-app/cdk.out cdk.out",
    "cdk:deploy": "lerna run deploy --stream --scope @typescript-backend-cdk-starter/cdk-app",
    "cdk:deploy:demo": "lerna run deploy:demo --stream --scope @typescript-backend-cdk-starter/cdk-app",
    "stop": "kill-port 3000",
    "lint": "yarn eslint './{utils,packages}/**/src/**/*.ts'"
  },
  "author": "Simon Verhoeven",
  "dependencies": {
    "@types/jest": "^29.5.3",
    "@types/uuid": "^9.0.2",
    "express": "^4.18.2",
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lerna": "^7.1.4",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-extra-plugin-user-data-dir": "^2.4.1",
    "puppeteer-extra-plugin-user-preferences": "^2.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "resolutions": {
    "aws-sdk": "2.965.0"
  },
  "lint-staged": {
    "{packages,utils}/*/src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --runInBand --findRelatedTests"
    ],
    "{packages,utils}/*/src/**/*.{json,ts,tsx,html,css,scss}": [
      "prettier --write --config ./.prettierrc"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && lerna run precommit --parallel --since HEAD"
    }
  },
  "packageManager": "yarn@3.6.3"
}
