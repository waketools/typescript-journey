    "aws-sdk": "^2.1430.0",
    "constructs": "^10.2.69",
    "jest": "^29.6.2",
    "json-beautify": "^1.1.1",
    "localstack": "^1.0.0",
    "lodash": "^4.17.21",
    "src": "link:./src",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
